// The Apple Developer account's Team ID
DEVELOPMENT_TEAM = HKE973VLUW

// The app bundle identifier
APP_ID = com.duckduckgo.mobile.ios

// A prefix for group ids. Must start with "group.".
GROUP_ID_PREFIX = group.com.duckduckgo

// The code signing style needs to be specified manually because the DDG team has multiple provisioning profiles that match the team and bundle identifier
CODE_SIGN_STYLE = Manual

// The manually specified provisioning profile
PROVISIONING_PROFILE_SPECIFIER = Development - App
